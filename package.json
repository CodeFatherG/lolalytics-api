{
  "name": "lolalytics-api",
  "version": "0.1.0",
  "description": "TypeScript library for scraping LoLalytics League of Legends statistics",
  "main": "./dist/cjs/api/index.js",
  "module": "./dist/esm/api/index.js",
  "types": "./dist/types/api/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/api/index.js",
      "import": "./dist/esm/api/index.js",
      "types": "./dist/types/api/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "lolalytics",
    "league-of-legends",
    "lol",
    "statistics",
    "scraping",
    "champion-stats",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0",
    "pino": "^9.0.0",
    "undici": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.7.2"
  }
}
